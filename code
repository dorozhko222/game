           if gh == '–∞':
                 moyo_xp -= uron_bossa
                 xp_bossa -= damage2
                 print('–í–∞—â–µ –∑–¥–æ—Ä–æ–≤—å–µ -',moyo_xp,'‚ù§Ô∏è')
                 print('–ó–¥–æ—Ä–æ–≤—å–µ –±–æ—Å—Å–∞',xp_bossa,'‚ù§Ô∏è')
