import random
import time
rand = random.randint(1,3)
damage = rand
damage2 = random.randint(4,6)
weapons = ['–±–∞–∑—É–∫–∞', '–±–∞–±–æ—á–∫–∞ "Legacy"', '–∞–ª–º–∞–∑–Ω—ã–π –º–µ—á','–ª–µ–∑–≤–∏–µ','–¥–∏–Ω–∞–º–∏—Ç–Ω–∞—è —à–∞—à–∫–∞','–¥—Ä–æ–±–æ–≤–∏–∫','']
pers = ['–∫—Ä—ã—Å–∞-–º—É—Ç–∞–Ω—Ç','–∫–æ–≥—Ç–∏',5,1.5,'–¥–µ–º–æ–Ω-—É–±–∏–π—Ü–∞','—Ç—Ä–µ–∑—É–±–µ—Ü',15,2,'–Ω–∏–Ω–∑—è','–∫–µ—Ä–∞–º–±–∏—Ç',8,1.5,'–≤–æ—Ä','–Ω–æ–∂', 15 ,2]
boss = ['–∫–æ—Ç-–º—É—Ç–∞–Ω—Ç',20,1,'–º–∞–≥',30,3,'–ø–∞—Ü–∞–Ω',20,2,'–æ—Ö—Ä–∞–Ω–Ω–∏–∫',50,2,]
def rob():
     print('–£–∫—Ä–∞–¥–∏ 1000 $')
     money = 0
     for i in range(10):
          if money >= 1000:
               print('–í–æ—Ä –¥–æ—Å—Ç–∏–≥ –∂–µ–ª–∞–µ–º–æ–≥–æ')
               break
          tea = input('–∫—Ä–∞—Å—Ç—å(–Ω–∞–∂–º–∏ –µ)')
          if tea == '–µ':
               money += 100
               print(money,'$')

def case():
     weapon = random.choice(weapons)
     damage = damage2
     print(weapon,',—É—Ä–æ–Ω -',damage,'‚ù§Ô∏è')

def fight():
     moyo_xp = xp_moyo
     xp_bossa = xp_boss
