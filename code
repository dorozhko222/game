               nums = '12345'
               code = ''
               for x in range(4):
                    code += num[random.randint(0, len(num) - 1)]
               for i in range(3):
                    time.sleep(1)
                    print('.')
               print('–í—ã –Ω–∞–∫–æ–Ω–µ—Ü —Ç–æ –Ω–∞—à–ª–∏ –∫–æ–¥ -',code)
               i = input('–í–≤–µ–¥–∏—Ç–µ –∫–æ–¥:')
               if i == code:
                    print('–°–µ–π—Ñ –æ—Ç–∫—Ä—ã—Ç!!!')
                    rob()
